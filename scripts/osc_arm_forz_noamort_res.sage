t,omega,F0=var('t,omega,F0')
A,B=var('A,B')                   
x=t*(A*cos(omega*t)+B*sin(omega*t))
eq=x.diff(t,2)+omega^2*x-F0*cos(omega*t)
eqL1=eq.coefficient(sin(omega*t))
eqL2=eq.coefficient(cos(omega*t))
SolAB=solve([eqL1,eqL2],[A,B],solution_dict=True)    
x=x.subs(SolAB[0])
show(x)
x.subs({omega:1,F0:1}).plot(t,0,100).show()
