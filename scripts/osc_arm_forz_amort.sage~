t,mu,omega,omega0,F0=var('t,mu,omega,omega0,F0')
A,B=var('A,B')                   
x=A*cos(omega0*t)+B*sin(omega0*t)
eq=x.diff(t,2)+2*mu*x.diff(t)+omega^2*x-F0*cos(omega0*t)
eqL1=eq.coefficient(sin(omega0*t))
eqL2=eq.coefficient(cos(omega0*t))
CoefSis=matrix([[eqL1.coefficient(A),eqL1.coefficient(B)],[eqL2.coefficient(A),eqL2.coefficient(B)]])
show(CoefSis.det()) 
SolAB=solve([eqL1,eqL2],[A,B],solution_dict=True)    
show(x.subs(SolAB[0]))
x=x.subs(SolAB[0][0])
#show(x)